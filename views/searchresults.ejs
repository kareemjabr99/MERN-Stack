<!DOCTYPE html>
<html lang="en">
<head>
    <style>
        body {
            background-image: url("background3.jpg");
            background-repeat: no-repeat;
            background-size: 100%;
        }

        h1 {
            color: white;
            text-align: center;
            -webkit-text-stroke: 1px blue;
        }
    </style>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Search Results</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
        integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark mb-2">
        <a class="navbar-brand" style="color: rgb(191, 255, 161)">Search Results</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
            </ul>
            <form method="POST" action='/search' class="form-inline my-2 my-lg-0">
                <input class="form-control mr-sm-2" name="Search" type="search" placeholder="Search"
                    aria-label="Search">
                <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
            </form>
        </div>
    </nav>
    <main>
    
        <% if (destinations.length > 0) { %>

            <%if(destinations.includes("home")){%>
                <a href="\home">home</a>
                <%}%>
            <br/>

            <%if(destinations.includes("islands")){%>
                <a href="\islands">islands</a>
                <%}%>
            <br/>
            

            <%if(destinations.includes("hiking")){%>
                <a href="\hiking">hiking</a>
                <%}%>
            <br/>
            
            <%if(destinations.includes("cities")){%>
                <a href="\cities">cities</a>
                <%}%>
            </br>
            
            <%if(destinations.includes("wantogo")){%>
                <a href="\wanttogo">wanttogo</a>
                <%}%>
            <br/>

            <%if(destinations.includes("annapurna")){%>
                <a href="\annapurna">Annapurna</a>
                <%}%>
            <br/>
            
            <%if(destinations.includes("inca")){%>
                <a href="\inca">Inca</a>
                <%}%>
            <br/>
            
            <%if(destinations.includes("rome")){%>
                <a href="\rome">Rome</a>
                <%}%>
            <br/>

            <%if(destinations.includes("paris")){%>
                <a href="/paris">Paris</a>
                <%}%>
            <br/>

            <%if(destinations.includes("bali")){%>
                <a href="\bali">Bali</a>
                <%}%> 
            <br/>

            <%if(destinations.includes("santorini")){%>    
                <a href="\santorini">Santorini</a>
                <%}%>
            <br/>
            
            <% if(destinations.includes("searchresults")) { %>
                <a href="\searchresults">searchresults</a>
                <% } %>
            <br/>
        <% } else { %>
            <h1 style="color: rgb(255, 255, 255)">Destinations not found</h1>
        <% } %>
    
    </main>
</body>
</html>